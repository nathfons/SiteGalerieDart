{% extends 'basemenu.html.twig' %}

{% block title %}Artiste{% endblock %}

{% block contentPage %}
    <h1>Artiste</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ artiste.id }}</td>
            </tr>
            <tr>
                <th>Biography</th>
                <td>{{ artiste.biography }}</td>
            </tr>
            <tr>
                <th>Approuve</th>
                <td>{{ artiste.approuve ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Photographie</th>
                <td>{{ artiste.photographie }}</td>
            </tr>
            <tr>
                <th>Miniature</th>
                <td>{{ artiste.miniature }}</td>
            </tr>
            <tr>
                <th>Commission</th>
                <td>{{ artiste.commission }}</td>
            </tr>
            <tr>
                <th>Alaune</th>
                <td>{{ artiste.alaune ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>TextAlaune</th>
                <td>{{ artiste.textAlaune }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ artiste.nom }}</td>
            </tr>
            <tr>
                <th>Prenom</th>
                <td>{{ artiste.prenom }}</td>
            </tr>
            <tr>
                <th>Telephone</th>
                <td>{{ artiste.telephone }}</td>
            </tr>
            <tr>
                <th>DateCreationCompte</th>
                <td>{{ artiste.dateCreationCompte ? artiste.dateCreationCompte|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('artiste_index') }}">back to list</a>

    <a href="{{ path('artiste_edit', {'id': artiste.id}) }}">edit</a>

    {{ include('artiste/_delete_form.html.twig') }}
{% endblock %}
